{
  "author": "Ben Gourley",
  "name": "cf-api",
  "description": "A pluggable server for HTTP APIs",
  "version": "0.1.0",
  "tags": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:clocklimited/cf-api"
  },
  "publishConfig": {
    "registry": "http://npm.clockte.ch"
  },
  "main": "api.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint . --reporter=./node_modules/jshint-full-path/index.js",
    "pretest": "npm run-script lint",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec --recursive",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage",
    "prepublish": "npm test && npm prune"
  },
  "engines": {
    "node": ">=0.8"
  },
  "dependencies": {
    "async": "~0.2.9",
    "express": "~3.4.4",
    "lodash": "~2.3.0"
  },
  "devDependencies": {
    "mocha": "1",
    "should": "2",
    "istanbul": "0",
    "jshint": "2",
    "jshint-full-path": "0",
    "supertest": "~0.8.1"
  }
}
